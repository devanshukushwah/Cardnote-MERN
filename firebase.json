{
  "hosting": {
    "public": "/client/build",
    "predeploy":["cd client","npm install","npm run build"],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
